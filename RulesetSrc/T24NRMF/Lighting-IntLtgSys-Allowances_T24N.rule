RULE NEW IntLtgSys:TMTaskMax
  RULESETS
    T24N_2022
  DATATYPE
    Float
 LONGFORM
    TailoredMethodTaskMaximum
  DESCRIPTION
    "Maximum LtgSys Specific Tailored Task Lighting Allowance (Watts)"
  INPUTCLASS 
    NotInput          
  UNITS 
    Watts
  DEFAULT
    if(IfValidAnd(IntLtgSys:AllowType = "TailoredTaskAllowance") .AND.
       ParentStatus(SpcFunc) > 0 .AND.
       Parent(SpcFunc) <> "- specify -")then
      if(ParentComp = "Spc")then
        TailoredMethodLPA:FlrDispAndTaskPwr("SpaceFunction", Parent(SpcFunc)) * AllowArea
      else
        TailoredMethodLPA:TaskLtgPwr("SpaceFunction", Parent(SpcFunc)) * AllowArea
      endif
    else
      0
    endif
