RULE NEW IntLtgSys:TMWallDispMax
  RULESETS
    T24N_2022
  DATATYPE
    Float
  LONGFORM
    TailoredMethodWallDisplayMaximum
  DESCRIPTION
    "Maximum LtgSys Specific Tailored Wall Display Allowance (Watts)"
  INPUTCLASS 
    NotInput    
  UNITS 
    Watts
  DEFAULT
    if(IfValidAnd(IntLtgSys:AllowType = "TailoredWallDisplayAllowance") .AND.
       ParentStatus(SpcFunc) > 0 .AND.
       Parent(SpcFunc) <> "- specify -")then
      if(ParentComp = "Spc")then
        TailoredMethodLPA:WallDispPwr("SpaceFunction", Parent(SpcFunc)) * TMWallFlrDispHtMult * AllowLen
      else
        TailoredMethodLPA:WallLtgPwr("SpaceFunction", Parent(SpcFunc)) * TMWallFlrDispHtMult * AllowLen
      endif
    else
      0
    endif
ENDRULE
