// ---------- Tailored Allowances - IntLtgSys ---------------------------------
RULE IntLtgSys:TMWallFlrDispHtMult
  RULESETS
    T24N_2022
  DESCRIPTION
    "Luminaire Height Multiplier for Wall and Floor Display Allowances"
  INPUTCLASS 
    NotInput
  MINIMUM 
    0
  MAXIMUM 
    20
  UNITS 
    ft
  DEFAULT
    if(IfValidAnd(IntLtgSys:LumMntgHgt > 18))then
      1 / 0.70
    else if(IfValidAnd(IntLtgSys:LumMntgHgt > 14))then
      1 / 0.75
    else if(IfValidAnd(IntLtgSys:LumMntgHgt > 10.5))then
      1 / 0.85
    else
      1.00
    endif endif endif
ENDRULE
RULE NEW IntLtgSys:TMWallDispAllow 
  RULESETS
    T24N_2022
  DATATYPE
    Float
  LONGFORM
    TailoredMethodWallDisplayAllowance
  DESCRIPTION
    "LtgSys Specific Tailored Wall Display Allowance (Watts)"
  INPUTCLASS 
    NotInput  
