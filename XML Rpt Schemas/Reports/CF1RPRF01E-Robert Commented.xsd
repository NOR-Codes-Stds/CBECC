<?xml version="1.0" encoding="UTF-8"?>
<!-- edited with XMLSpy v2015 sp2 (http://www.altova.com) by Diane Pepetone (L'Monte Information Services) -->
<xsd:schema xmlns="http://www.lmonte.com/besm/CF1RPRF01E" xmlns:d="http://www.lmonte.com/besm/d" xmlns:bld="http://www.lmonte.com/besm/bld" xmlns:env="http://www.lmonte.com/besm/env" xmlns:lit="http://www.lmonte.com/besm/lit" xmlns:hvac="http://www.lmonte.com/besm/hvac" xmlns:comp="http://www.lmonte.com/besm/comp" xmlns:com="http://www.lmonte.com/besm/com" xmlns:dtyp="http://www.lmonte.com/besm/dtyp" xmlns:xsd="http://www.w3.org/2001/XMLSchema" xmlns:altova="http://www.altova.com/xml-schema-extensions" targetNamespace="http://www.lmonte.com/besm/CF1RPRF01E" elementFormDefault="qualified" attributeFormDefault="unqualified" version="2016.1.007">
	<xsd:import namespace="http://www.lmonte.com/besm/com" schemaLocation="../base/ResCommon.xsd"/>
	<xsd:import namespace="http://www.lmonte.com/besm/bld" schemaLocation="../base/ResBuilding.xsd"/>
	<xsd:import namespace="http://www.lmonte.com/besm/env" schemaLocation="../base/ResEnvelope.xsd"/>
	<xsd:import namespace="http://www.lmonte.com/besm/lit" schemaLocation="../base/ResLighting.xsd"/>
	<xsd:import namespace="http://www.lmonte.com/besm/hvac" schemaLocation="../base/ResHvac.xsd"/>
	<xsd:import namespace="http://www.lmonte.com/besm/comp" schemaLocation="../base/ResCompliance.xsd"/>
	<xsd:import namespace="http://www.lmonte.com/besm/dtyp" schemaLocation="../base/DataTypes.xsd"/>
	<xsd:element name="ComplianceDocumentPackage">
		<xsd:complexType>
			<xsd:sequence>
				<xsd:element name="DocID" minOccurs="0">
					<xsd:complexType>
						<xsd:attribute name="doc" type="comp:ComplianceDocumentTag" fixed="CF1RPRF01E"/>
						<xsd:attribute name="docType" type="comp:ComplianceDocumentType" fixed="CERTIFICATE OF COMPLIANCE -  RESIDENTIAL PERFORMANCE COMPLIANCE METHOD"/>
						<xsd:attribute name="docTitle" type="comp:ComplianceDocumentTitleRes" fixed="Residential Performance Compliance Method"/>
					</xsd:complexType>
				</xsd:element>
				<xsd:element name="Payload" type="comp:Payload"/>
				<xsd:element name="DocumentData">
					<xsd:complexType>
						<xsd:sequence>
							<xsd:element name="Header" type="comp:HeaderCF1R"/>
							<xsd:element ref="cF1RPRF01E"/>
							<xsd:element name="DocAuthor" type="comp:DocumentAuthor"/>
							<xsd:element name="RespPerson" type="comp:ResponsiblePersonCF1R"/>
							<xsd:element name="Footer" type="comp:Footer"/>
						</xsd:sequence>
					</xsd:complexType>
				</xsd:element>
				<xsd:element name="Report" type="xsd:base64Binary"/>
			</xsd:sequence>
			<xsd:attribute name="revision" type="xsd:string" use="required" fixed="rev 2017-12-01"/>
			<xsd:attribute name="doc" type="comp:ComplianceDocumentTag" use="required" fixed="CF1RPRF01E"/>
		</xsd:complexType>
	</xsd:element>
	<xsd:complexType name="CF1RPRF01E">
		<xsd:annotation>
			<xsd:documentation>Residential Performance Compliance Method</xsd:documentation>
		</xsd:annotation>
		<xsd:sequence>
			<xsd:element name="Section_A" minOccurs="1">
				<xsd:annotation>
					<xsd:documentation source="FieldText">GENERAL INFORMATION</xsd:documentation>
					<xsd:documentation source="MinOccurs">This section is required.</xsd:documentation>
				</xsd:annotation>
				<xsd:complexType>
					<xsd:sequence>
						<xsd:element name="A01_ProjectName" type="com:ProjectName">
							<xsd:annotation>
								<xsd:documentation source="FieldText">Project Name</xsd:documentation>
							</xsd:annotation>
						</xsd:element>
						<xsd:element name="A02_RunDescription" type="comp:RunDescription">
							<xsd:annotation>
								<xsd:documentation source="FieldText">Calculation Description</xsd:documentation>
							</xsd:annotation>
						</xsd:element>
						<xsd:element name="A03_StreetAddress" type="com:StreetAddress">
							<xsd:annotation>
								<xsd:documentation source="FieldText">Project Location</xsd:documentation>
							</xsd:annotation>
						</xsd:element>
						<xsd:sequence maxOccurs="1">
							<xsd:annotation/>
							<xsd:element name="A04_City" type="comp:City">
								<xsd:annotation>
									<xsd:documentation source="FieldText">City</xsd:documentation>
								</xsd:annotation>
							</xsd:element>
							<xsd:element name="A04_State" type="comp:State" minOccurs="0" maxOccurs="1">
								<xsd:annotation>
									<xsd:documentation source="FieldText">City</xsd:documentation>
								</xsd:annotation>
							</xsd:element>
						</xsd:sequence>
						<xsd:element name="A05_Title24StandardsVersion" type="comp:Title24StandardsVersion">
							<xsd:annotation>
								<xsd:documentation source="FieldText">Standards Version</xsd:documentation>
							</xsd:annotation>
						</xsd:element>
						<xsd:element name="A06_Zipcode" type="comp:Zipcode" minOccurs="0">
							<xsd:annotation>
								<xsd:documentation source="FieldText">Zip code</xsd:documentation>
							</xsd:annotation>
						</xsd:element>
						<xsd:element name="A07_ComplianceSoftwareVersion" type="comp:ComplianceSoftwareVersion">
							<xsd:annotation>
								<xsd:documentation source="FieldText">Software Version</xsd:documentation>
							</xsd:annotation>
						</xsd:element>
						<xsd:element name="A08_ClimateZone" type="com:ClimateZone">
							<xsd:annotation>
								<xsd:documentation source="FieldText">Climate Zone</xsd:documentation>
							</xsd:annotation>
						</xsd:element>
						<xsd:choice maxOccurs="1">
							<xsd:annotation>
								<xsd:documentation source="FieldText">Front Orientation (deg/ Cardinal)</xsd:documentation>
							</xsd:annotation>
							<xsd:element name="A09_Azimuth" type="dtyp:Azimuth"/>
							<xsd:element name="A09_CardinalOrientation" type="com:CardinalOrientation"/>
						</xsd:choice>
						<xsd:element name="A10_ResidentialLowriseBuildingType" type="com:ResidentialLowriseBuildingType">
							<xsd:annotation>
								<xsd:documentation source="FieldText">Building Type</xsd:documentation>
							</xsd:annotation>
						</xsd:element>
						<xsd:element name="A11_DwellingUnitCount" type="com:DwellingUnitCount">
							<xsd:annotation>
								<xsd:documentation source="FieldText">Number of Dwelling Units</xsd:documentation>
							</xsd:annotation>
						</xsd:element>
						<xsd:element name="A12_ProjectScopePerformance" type="comp:ProjectScopePerformance">
							<xsd:annotation>
								<xsd:documentation source="FieldText">Project Scope</xsd:documentation>
							</xsd:annotation>
						</xsd:element>
						<xsd:element name="A13_BedroomCount" type="com:BedroomCount">
							<xsd:annotation>
								<xsd:documentation source="FieldText">Number of Bedrooms</xsd:documentation>
							</xsd:annotation>
						</xsd:element>
						<xsd:element name="A14_NewAdditionConditionedFloorArea" type="bld:NewAdditionConditionedFloorArea">
							<xsd:annotation>
								<xsd:documentation source="FieldText">New Construction/Addition Cond. Floor Area (ft[d:sup]2[/d:sup])</xsd:documentation>
							</xsd:annotation>
						</xsd:element>
						<xsd:element name="A15_TotalStoryCount" type="com:TotalStoryCount">
							<xsd:annotation>
								<xsd:documentation source="FieldText">Number of Stories</xsd:documentation>
							</xsd:annotation>
						</xsd:element>
						<xsd:element name="A16_ExistingConditionedFloorArea" type="bld:ExistingConditionedFloorArea">
							<xsd:annotation>
								<xsd:documentation source="FieldText">Existing Cond. Floor Area (ft[d:sup]2[/d:sup])</xsd:documentation>
							</xsd:annotation>
						</xsd:element>
						<xsd:element name="A17_NaturalGasOnSite" type="com:NaturalGasOnSite">
							<xsd:annotation>
								<xsd:documentation source="FieldText">Natural Gas Available</xsd:documentation>
							</xsd:annotation>
						</xsd:element>
						<xsd:choice maxOccurs="1">
							<xsd:annotation>
								<xsd:documentation source="FieldText">Total Cond. Floor Area (ft[d:sup]2[/d:sup])</xsd:documentation>
							</xsd:annotation>
							<xsd:element name="A18_TotalConditionedFloorArea" type="com:TotalConditionedFloorArea"/>
							<xsd:element name="A18_NotApplicableMessage" type="comp:NotApplicableMessage"/>
						</xsd:choice>
						<xsd:element name="A19_FenestrationFloorRatio" type="env:FenestrationFloorRatio">
							<xsd:annotation>
								<xsd:documentation source="FieldText">Glazing Percentage (%)</xsd:documentation>
								<xsd:documentation source="CalculationsAndRules">Display text is value x 100 followed by the percent symbol,  %</xsd:documentation>
							</xsd:annotation>
						</xsd:element>
					</xsd:sequence>
				</xsd:complexType>
			</xsd:element>
			<xsd:element name="Section_B" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation source="FieldText">ADDITION ALONE PROJECT ANALYSIS PARAMETERS</xsd:documentation>
					<xsd:documentation source="MinOccurs">If this section doesn't apply, display only the section FieldText and the statement 'This section does not apply to this project.'</xsd:documentation>
				</xsd:annotation>
				<xsd:complexType>
					<xsd:sequence>
						<xsd:element name="B01_FloorArea" type="com:FloorArea">
							<xsd:annotation>
								<xsd:documentation source="FieldText">Existing Area (excl. new addition) (ft[d:sup]2[/d:sup])</xsd:documentation>
							</xsd:annotation>
						</xsd:element>
						<xsd:element name="B02_FloorArea" type="com:FloorArea">
							<xsd:annotation>
								<xsd:documentation source="FieldText">Addition Area (excl. existing) (ft[d:sup]2[/d:sup])</xsd:documentation>
							</xsd:annotation>
						</xsd:element>
						<xsd:element name="B03_FloorArea" type="com:FloorArea">
							<xsd:annotation>
								<xsd:documentation source="FieldText">Total Area  (ft[d:sup]2[/d:sup])</xsd:documentation>
							</xsd:annotation>
						</xsd:element>
						<xsd:element name="B04_BedroomsExistingCount" type="com:BedroomsExistingCount">
							<xsd:annotation>
								<xsd:documentation source="FieldText">Existing Bedrooms</xsd:documentation>
							</xsd:annotation>
						</xsd:element>
						<xsd:element name="B05_BedroomsAdditionCount" type="com:BedroomsAdditionCount">
							<xsd:annotation>
								<xsd:documentation source="FieldText">Addition Bedrooms</xsd:documentation>
							</xsd:annotation>
						</xsd:element>
						<xsd:element name="B06_BedroomCount" type="com:BedroomCount">
							<xsd:annotation>
								<xsd:documentation source="FieldText">Total Bedrooms</xsd:documentation>
							</xsd:annotation>
						</xsd:element>
					</xsd:sequence>
				</xsd:complexType>
			</xsd:element>
			<xsd:element name="Section_C" minOccurs="1">
				<xsd:annotation>
					<xsd:documentation source="FieldText">COMPLIANCE RESULTS</xsd:documentation>
					<xsd:documentation source="MinOccurs">This section is required.</xsd:documentation>
				</xsd:annotation>
				<xsd:complexType>
					<xsd:sequence>
						<xsd:element name="C01_ResidentialPerformanceComplianceResult" type="comp:ResidentialPerformanceComplianceResult">
							<xsd:annotation>
								<xsd:documentation source="FieldText">01</xsd:documentation>
							</xsd:annotation>
						</xsd:element>
						<xsd:element name="C02_ResidentialPerformanceHERS_VerificationRequired" type="comp:ResidentialPerformanceHERS_VerificationRequired">
							<xsd:annotation>
								<xsd:documentation source="FieldText">02</xsd:documentation>
							</xsd:annotation>
						</xsd:element>
						<xsd:element name="C03_ResidentialPerformanceSpecialFeatures" type="comp:ResidentialPerformanceSpecialFeatures">
							<xsd:annotation>
								<xsd:documentation source="FieldText">03</xsd:documentation>
								<xsd:documentation source="CalculationsAndRules">This field value is only displayed if ResidentialPerformanceSpecialFeatures is true.  If true the display text is This building incorporates one or more Special Features shown below.</xsd:documentation>
							</xsd:annotation>
						</xsd:element>
					</xsd:sequence>
				</xsd:complexType>
			</xsd:element>
			<xsd:element name="Section_C04To08" minOccurs="1">
				<xsd:annotation>
					<xsd:documentation source="FieldText">ENERGY USE SUMMARY</xsd:documentation>
					<xsd:documentation source="CalculationsAndRules">This section is applicable if value in 1-11is the CardinalOrientation property not equal to AllOrientations</xsd:documentation>
					<xsd:documentation source="MinOccurs">This section is required.</xsd:documentation>
				</xsd:annotation>
				<xsd:complexType>
					<xsd:sequence>
						<xsd:element name="C04" minOccurs="0">
							<xsd:annotation>
								<xsd:documentation source="FieldText">Energy Use (kTDV/ft2-yr)</xsd:documentation>
							</xsd:annotation>
						</xsd:element>
						<xsd:element name="C04Row1Header" minOccurs="0">
							<xsd:annotation>
								<xsd:documentation source="FieldText"/>
								<xsd:documentation source="AdditionalRequirements">Space Heating</xsd:documentation>
							</xsd:annotation>
						</xsd:element>
						<xsd:element name="C04Row2Header" minOccurs="0">
							<xsd:annotation>
								<xsd:documentation source="FieldText"/>
								<xsd:documentation source="AdditionalRequirements">Space Cooling</xsd:documentation>
							</xsd:annotation>
						</xsd:element>
						<xsd:element name="C04Row3Header" minOccurs="0">
							<xsd:annotation>
								<xsd:documentation source="FieldText"/>
								<xsd:documentation source="AdditionalRequirements">IAQ Ventilation</xsd:documentation>
							</xsd:annotation>
						</xsd:element>
						<xsd:element name="C04Row4Header" minOccurs="0">
							<xsd:annotation>
								<xsd:documentation source="FieldText"/>
								<xsd:documentation source="AdditionalRequirements">Water Heating</xsd:documentation>
							</xsd:annotation>
						</xsd:element>
						<xsd:element name="C04Row5Header" minOccurs="0">
							<xsd:annotation>
								<xsd:documentation source="FieldText"/>
								<xsd:documentation source="AdditionalRequirements">Photovoltaic Offset</xsd:documentation>
							</xsd:annotation>
						</xsd:element>
						<xsd:element name="C04Row6Header" minOccurs="0">
							<xsd:annotation>
								<xsd:documentation source="FieldText"/>
								<xsd:documentation source="AdditionalRequirements">Compliance Ernergy Total</xsd:documentation>
							</xsd:annotation>
						</xsd:element>
						<xsd:element name="Table">
							<xsd:complexType>
								<xsd:sequence>
									<xsd:element name="Row" minOccurs="6" maxOccurs="6">
										<xsd:complexType>
											<xsd:sequence>
												<xsd:element name="C05_EnergyUseStandardDesign" type="comp:EnergyUseStandardDesign">
													<xsd:annotation>
														<xsd:documentation source="FieldText">Standard Design</xsd:documentation>
														<xsd:documentation source="CalculationsAndRules">If  Row 5 Photovoltaic Offset, there is no Standard Design value.  Display "-----"</xsd:documentation>
													</xsd:annotation>
												</xsd:element>
												<xsd:element name="C06_EnergyUseProposedDesign" type="comp:EnergyUseProposedDesign">
													<xsd:annotation>
														<xsd:documentation source="FieldText">Proposed Design</xsd:documentation>
													</xsd:annotation>
												</xsd:element>
												<xsd:element name="C07_EnergyUseComplianceMargin" type="comp:EnergyUseComplianceMargin">
													<xsd:annotation>
														<xsd:documentation source="FieldText">Compliance Margin</xsd:documentation>
													</xsd:annotation>
												</xsd:element>
												<xsd:element name="C08_EnergyUsePercentImprovement" type="comp:EnergyUsePercentImprovement">
													<xsd:annotation>
														<xsd:documentation source="FieldText">Percent Improvement</xsd:documentation>
														<xsd:documentation source="CalculationsAndRules">If  Row 5 Photovoltaic Offset, there is no Percent Improvement. Display "-----"</xsd:documentation>
													</xsd:annotation>
												</xsd:element>
											</xsd:sequence>
										</xsd:complexType>
									</xsd:element>
								</xsd:sequence>
							</xsd:complexType>
						</xsd:element>
						<xsd:element name="C09_ProjectNarrative" type="comp:ProjectNarrative">
							<xsd:annotation>
								<xsd:documentation source="FieldText">ADDITIONAL REMARKS</xsd:documentation>
							</xsd:annotation>
						</xsd:element>
					</xsd:sequence>
				</xsd:complexType>
			</xsd:element>
			<!-- Special features are contained in a single element and are likely to be included in the type described below -->
			<xsd:element name="Section_D" minOccurs="1">
				<xsd:annotation>
					<xsd:documentation source="FieldText">REQUIRED SPECIAL FEATURES</xsd:documentation>
					<xsd:documentation source="MinOccurs">This section is required.</xsd:documentation>
				</xsd:annotation>
				<xsd:complexType>
					<xsd:sequence>
						<xsd:element name="D_BeginNote1" minOccurs="0">
							<xsd:annotation>
								<xsd:documentation source="FieldText">Begin Note 1</xsd:documentation>
								<xsd:documentation source="AdditionalRequirements">The following are features that must be installed as condition for meeting the modeled energy performance for this computer analysis.</xsd:documentation>
							</xsd:annotation>
						</xsd:element>
						<xsd:sequence maxOccurs="unbounded">
							<xsd:annotation>
								<xsd:documentation source="FieldText"/>
							</xsd:annotation>
							<xsd:element name="D01_SpecialFeatureCategory" type="comp:SpecialFeatureCategory"/>
						</xsd:sequence>
					</xsd:sequence>
				</xsd:complexType>
			</xsd:element>
			
			<!-- This section has multiple categories in the existing xml that can have multiple sections -->
			<xsd:element name="Section_E" minOccurs="1">
				<xsd:annotation>
					<xsd:documentation source="FieldText">HERS FEATURE SUMMARY</xsd:documentation>
					<xsd:documentation source="MinOccurs">This section is required.</xsd:documentation>
				</xsd:annotation>
				<xsd:complexType>
					<xsd:sequence>
						<xsd:element name="E_BeginNote1" minOccurs="0">
							<xsd:annotation>
								<xsd:documentation source="FieldText">Begin Note 1</xsd:documentation>
								<xsd:documentation source="AdditionalRequirements">The following is a summary of the  features that must be field-verified by a certified HERS Rater as a condition for meeting the modeled energy performance for this computer analysis.  Additional detail is provided in the buildng tables below.</xsd:documentation>
							</xsd:annotation>
						</xsd:element>
						<xsd:sequence maxOccurs="unbounded">
							<xsd:annotation>
								<xsd:documentation source="FieldText">Building-level Verifications:</xsd:documentation>
							</xsd:annotation>
							<xsd:element name="E01_HERS_FeatureBuilding" type="comp:HERS_FeatureBuilding"/>
						</xsd:sequence>
						<xsd:sequence maxOccurs="unbounded">
							<xsd:annotation>
								<xsd:documentation source="FieldText">Cooling System Verifications:</xsd:documentation>
							</xsd:annotation>
							<xsd:element name="E02_HERS_FeatureCoolingSystem" type="comp:HERS_FeatureCoolingSystem"/>
						</xsd:sequence>
						<xsd:sequence maxOccurs="unbounded">
							<xsd:annotation>
								<xsd:documentation source="FieldText">HVAC Distribution System Verifications:</xsd:documentation>
							</xsd:annotation>
							<xsd:element name="E03_HERS_FeatureHvacDistribution" type="comp:HERS_FeatureHvacDistribution"/>
						</xsd:sequence>
						<xsd:sequence maxOccurs="unbounded">
							<xsd:annotation>
								<xsd:documentation source="FieldText">Domestic Hot Water  System Verifications:</xsd:documentation>
							</xsd:annotation>
							<xsd:element name="E04_HERS_FeatureDomesticHotWater" type="comp:HERS_FeatureDomesticHotWater"/>
						</xsd:sequence>
					</xsd:sequence>
				</xsd:complexType>
			</xsd:element>
			<xsd:element name="Section_F" minOccurs="1">
				<xsd:annotation>
					<xsd:documentation source="FieldText">BUILDING - FEATURES INFORMATION</xsd:documentation>
					<xsd:documentation source="MinOccurs">This section is required.</xsd:documentation>
				</xsd:annotation>
				<xsd:complexType>
					<xsd:sequence>
						<xsd:element name="F01_ProjectName" type="com:ProjectName">
							<xsd:annotation>
								<xsd:documentation source="FieldText">Project Name</xsd:documentation>
								<xsd:documentation source="CalculationsAndRules">Auto-filled value from section 1 ProjectName</xsd:documentation>
							</xsd:annotation>
						</xsd:element>
						<xsd:element name="F02_ProjectConditionedFloorArea" type="comp:ProjectConditionedFloorArea">
							<xsd:annotation>
								<xsd:documentation source="FieldText">Conditioned Floor Area (ft[d:sup]2[/d:sup])</xsd:documentation>
								<xsd:documentation source="CalculationsAndRules">ISSUE: Is this is total CFA of the building, or the project?</xsd:documentation>
							</xsd:annotation>
						</xsd:element>
						<xsd:element name="F03_DwellingUnitCount" type="com:DwellingUnitCount">
							<xsd:annotation>
								<xsd:documentation source="FieldText">Number of Dwelling Units</xsd:documentation>
								<xsd:documentation source="CalculationsAndRules">Auto-filled value from 1-11 ResidentialDwellingUnitCount</xsd:documentation>
							</xsd:annotation>
						</xsd:element>
						<xsd:element name="F04_BedroomCount" type="com:BedroomCount">
							<xsd:annotation>
								<xsd:documentation source="FieldText">Number of Bedrooms</xsd:documentation>
								<xsd:documentation source="CalculationsAndRules">Auto-filled value from 1-13 BedroomCount</xsd:documentation>
							</xsd:annotation>
						</xsd:element>
						<xsd:element name="F05_ResidentialBuildingZoneCount" type="com:ResidentialBuildingZoneCount">
							<xsd:annotation>
								<xsd:documentation source="FieldText">Number of Zones</xsd:documentation>
							</xsd:annotation>
						</xsd:element>
						<xsd:element name="F06_ResidentialVentilationCoolingSystemCount" type="hvac:ResidentialVentilationCoolingSystemCount">
							<xsd:annotation>
								<xsd:documentation source="FieldText">Number of Ventilation Cooling Systems</xsd:documentation>
							</xsd:annotation>
						</xsd:element>
						<xsd:element name="F07_ResidentialWaterHeatingSystemCount" type="hvac:ResidentialWaterHeatingSystemCount">
							<xsd:annotation>
								<xsd:documentation source="FieldText">Number of Water Heating  Systems</xsd:documentation>
							</xsd:annotation>
						</xsd:element>
					</xsd:sequence>
				</xsd:complexType>
			</xsd:element>
			<!-- THere can be multiple zones so this should allow rows -->
			<xsd:element name="G_Choice">
				<xsd:annotation>
					<xsd:documentation source="FieldText">Choice Single Family or Multifamily Zone Information</xsd:documentation>
				</xsd:annotation>
				<xsd:complexType>
					<xsd:choice>
						<xsd:element name="Section_G" minOccurs="1">
							<xsd:annotation>
								<xsd:documentation source="FieldText">ZONE INFORMATION</xsd:documentation>
								<xsd:documentation source="CalculationsAndRules">This version of Zone Information table is present when Building type in General Information section equals SingleFamily</xsd:documentation>
								<xsd:documentation source="MinOccurs">This section is required.</xsd:documentation>
							</xsd:annotation>
							<xsd:complexType>
								<xsd:sequence>
									<xsd:element name="G01_ThermalZoneName" type="com:ThermalZoneName">
										<xsd:annotation>
											<xsd:documentation source="FieldText">Zone Name</xsd:documentation>
										</xsd:annotation>
									</xsd:element>
									<xsd:element name="G02_ZoneConditioningType">
										<xsd:annotation>
											<xsd:documentation source="FieldText">Zone Type</xsd:documentation>
										</xsd:annotation>
										<xsd:simpleType>
											<xsd:restriction base="hvac:ZoneConditioningType">
												<xsd:enumeration value="Conditioned"/>
												<xsd:enumeration value="Living"/>
												<xsd:enumeration value="Sleeping"/>
												<xsd:enumeration value="Unconditioned"/>
											</xsd:restriction>
										</xsd:simpleType>
									</xsd:element>
									<xsd:element name="G03_HvacSystemName" type="hvac:HvacSystemName">
										<xsd:annotation>
											<xsd:documentation source="FieldText">HVAC System Name</xsd:documentation>
										</xsd:annotation>
									</xsd:element>
									<xsd:element name="G04_ThermalZoneFloorArea" type="hvac:ThermalZoneFloorArea">
										<xsd:annotation>
											<xsd:documentation source="FieldText">Zone Floor Area (ft[d:sup]2[/d:sup])</xsd:documentation>
										</xsd:annotation>
									</xsd:element>
									<xsd:element name="G05_FloorToCeilingHeight" type="com:FloorToCeilingHeight">
										<xsd:annotation>
											<xsd:documentation source="FieldText">Avg. Ceiling Height</xsd:documentation>
										</xsd:annotation>
									</xsd:element>
									<xsd:element name="G06_WaterHeatingSystemName" type="hvac:WaterHeatingSystemName">
										<xsd:annotation>
											<xsd:documentation source="FieldText">Water Heating System 1</xsd:documentation>
